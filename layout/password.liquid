<body class="template-password">
  <div class="password-page text-center">
    <header class="password-header" role="banner">
      <div class="password-header__inner">
        <div class="password-login">
          <a href="#LoginModal" class="js-modal-open-login-modal link--action btn btn--secondary">
            {% comment %}ly_i18n_replace_for_[ 'general.password_page.password_link' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.password_link' | t }}{% endcapture %}{% capture ly_translation %}{% render 'ly-static-string' with '16902398' render:true %}{% endcapture %}{% if ly_translation != '' and ly_translation != nil  %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %} →
          </a>
        </div>
      </div>
    </header>

    <div class="password-main text-center" role="main">
      <div class="password-main__inner">
        {{ content_for_layout }}
      </div>
    </div>
  </div>

  <div id="LoginModal" class="modal">
    <div class="modal__inner">
      <div class="modal__centered password-modal__content page-width text-center">
        <p class="password__form-heading h4">{% comment %}ly_i18n_replace_for_[ 'general.password_page.login_form_heading' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.login_form_heading' | t }}{% endcapture %}{% capture ly_translation %}{% render 'ly-static-string' with '16902399' render:true %}{% endcapture %}{% if ly_translation != '' and ly_translation != nil  %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</p>
          {% form 'storefront_password' %}
            {{ form.errors | default_errors }}
            <label for="password" class="label--hidden">{% comment %}ly_i18n_replace_for_[ 'general.password_page.login_form_password_label' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.login_form_password_label' | t }}{% endcapture %}{% capture ly_translation %}{% render 'ly-static-string' with '16902400' render:true %}{% endcapture %}{% if ly_translation != '' and ly_translation != nil  %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</label>
            <div class="input-group password__input-group">
              <input type="password" name="password" id="Password" class="input-group__field input--content-color" placeholder="{% comment %}ly_i18n_replace_for_[ 'general.password_page.login_form_password_placeholder' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.login_form_password_placeholder' | t }}{% endcapture %}{% capture ly_translation %}{% render 'ly-static-string' with '16902401' render:true %}{% endcapture %}{% if ly_translation != '' and ly_translation != nil  %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}">
              <span class="input-group__btn">
                <button type="submit" name="commit" class="btn btn--narrow">
                  {% comment %}ly_i18n_replace_for_[ 'general.password_page.login_form_submit' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.login_form_submit' | t }}{% endcapture %}{% capture ly_translation %}{% render 'ly-static-string' with '16902402' render:true %}{% endcapture %}{% if ly_translation != '' and ly_translation != nil  %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}
                </button>
              </span>
            </div>
          {% endform %}
        <p>{% comment %}ly_i18n_replace_for_[ 'general.password_page.admin_link_html' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.password_page.admin_link_html' | t }}{% endcapture %}{% capture ly_translation %}{% render 'ly-static-string' with '16902403' render:true %}{% endcapture %}{% if ly_translation != '' and ly_translation != nil  %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</p>
      </div>
    </div>

    <button type="button" class="modal__close js-modal-close text-link">
      {% include 'icon-close' %}
      <span class="icon__fallback-text">{% comment %}ly_i18n_replace_for_[ 'general.accessibility.close_modal' | t | json ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.accessibility.close_modal' | t | json }}{% endcapture %}{% capture ly_translation %}{% render 'ly-static-string' with '16902404' render:true %}{% endcapture %}{% if ly_translation != '' and ly_translation != nil  %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape | json }}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</span>
    </button>
  </div>

  <script>
    var theme = {
      strings: {
        addressError: {{ 'sections.map.address_error' | t | json }},
        addressNoResults: {{ 'sections.map.address_no_results' | t | json }},
        addressQueryLimit: {{ 'sections.map.address_query_limit_html' | t | json }},
        authError: {{ 'sections.map.auth_error_html' | t | json }}
      }
    }
  </script>

{% comment %}ly_static_code_begin{% endcomment %}{% include 'ly-languages' %}{% comment %}ly_static_code_end{% endcomment %}</body>
