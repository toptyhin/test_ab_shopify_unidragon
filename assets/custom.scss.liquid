/***** custom-tax************/
.custom-tax {
  width: 100%;
  float: left;
  margin-top: 5px;
  margin-bottom: 5px;
}
/* Style the buttons that are used to open and close the accordion panel */
.custom-tax .accordion-tax {
  color: #000;
  cursor: pointer;
  width: 100%;
  font-weight:400;
  text-transform:uppercase;
  text-align: left;
  border: none;
  outline: none;
  transition: 0.4s;
}

/* Style the accordion panel. Note: hidden by default */
.custom-tax .panel {
  background-color: white;
  display: none;
  text-align: justify;
  overflow: hidden;
}
.custom-tax .accordion-tax:after {
  content: '\02795'; /* Unicode character for "plus" sign (+) */
  font-size: 13px;
  color: #000;
  float: right;
  margin-left: 5px;
}

.custom-tax .active:after {
  content: "\2796"; /* Unicode character for "minus" sign (-) */
}
