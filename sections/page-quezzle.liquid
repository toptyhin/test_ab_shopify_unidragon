<script>
  $('.AddToCart').on('click', function (e){    
    e.preventDefault();
    let formData = {
     'items': [{
       'id': e.target.dataset.variantId,
      'quantity': 1
      }]
    };
    fetch('/cart/add.js', {
      method: 'POST',
      headers: {
        'Content-Type': 'application/json'
      },
      body: JSON.stringify(formData)
    })
    .then(response => {
         $.ajax({
            type: 'GET',
            url: '../cart.json',
            dataType: "json",
           success: function (data) { $("#CartCost .money").text('$ '+data.total_price*0.01);}
          });
    })
    .catch((error) => {
      console.error('Error:', error);
    });
  
 });
  </script>
