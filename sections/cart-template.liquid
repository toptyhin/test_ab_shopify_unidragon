              var time_minute={{ section.settings.cart_timer_minute }}/60 ;
              var hours = Math.floor( time_minute / 60);
              var minutes = Math.floor(time_minute % 60);
              $(function(){
                $('#hm_timer120').countdowntimer({
                  hours : hours,
                  minutes :minutes,
                  seconds :00
                });
              });
