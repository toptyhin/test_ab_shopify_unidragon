    {% if section.settings.enable_sticky %}var eblstcky = true;{% else %}var eblstcky = false;{% endif %}{% if section.settings.enable_payment_button %}var ebl_pay_but = true;var addToCartBtnLabel_1 = "{{ 'products.dynamic_checkout_buttons.add_to_cart_text_when_dynamic_checkout_buttons_enable' | t }}";{% else %}var ebl_pay_but = false;var addToCartBtnLabel_1 = "{{ 'products.product.add_to_cart' | t }}";{% endif %}{% if section.settings.visitor_counter_enable %}var visitorcountr = true;{% else %}var visitorcountr = false;{% endif %}
  </script>
  
  <div class="product-template__container product-{{ product.id }} page-width" itemscope itemtype="http://schema.org/Product" id="ProductSection-{{ section.id }}" data-section-id="{{ section.id }}" data-section-type="product" data-enable-history-state="true"> 
 <div> {{ product.metafields.alireviews.seo_rating_review_key }} </div> 
  
    <meta itemprop="name" content="{{ product.title }}">
    <meta itemprop="url" content="{{ shop.url }}{{ product.url }}">
    <meta itemprop="image" content="{{ product.featured_image.src | img_url: '800x' }}">
    <meta itemprop="productID" content="{{product.id }}">
    {%- liquid
  
      assign current_variant = product.selected_or_first_available_variant
  
        for item in cart.items
          if item.product.type == 'Puzzle'
            assign puzle_discount = puzle_discount | plus: item.quantity
          endif
        endfor
  
        if customer
          for tag in customer.tags
            if tag == 'Subscription membership' 
              assign member_discount = true
            endif
          endfor
        endif
        
        assign discount_amount = 1
  
        if member_discount == true
          assign discount_amount =  0.8
        elsif puzle_discount > 2 and product.type == 'Puzzle'
          assign discount_amount =  0.9
        endif
  
      assign product_image_zoom_size = '1024x1024'
      assign product_image_scale = '2'
      assign enable_zoom = section.settings.enable_zoom 
    -%}
  
    <div class="grid product-single">
      <div class="grid__item product-single__photos medium-up--one-half">
        {%- render 'label-product-tag', products: product -%}
        {%- if section.settings.enable_default_variant -%}
        {%- assign featured_image = product.featured_image -%}
        {%- else -%}
        {%- assign featured_image = product.selected_or_first_available_variant.featured_image | default: product.featured_image -%}
        {%- endif -%}
        {%- assign videos = product.media | where: "media_type", "external_video" -%}
        {%- if videos.size > 0 -%}
          {%- for video in videos -%}
            <div id="{{ img_wrapper_id }}" class="product-single__photo-wrapper small--hide js videoframe">
              <div id="{{ zoom_img_id }}" style="padding-top:{{ 1 | divided_by: media.aspect_ratio | times: 100}}%; position: relative;overflow: hidden;" class="product-single__photo-{{ section.id }}{% if product.images.size > 1 %} product-single__photo--has-thumbnails{% endif %} hide" data-image-id="{{ video.id }}">                
                  {{ video | external_video_tag }}
              </div>
            </div>
          {%- endfor -%}
        {%- endif -%}
          {% for media in product.media %}
              {% case media.media_type %}
              {% when 'image' %}
                  {% capture img_id %}FeaturedImage-{{ section.id }}-{{ media.id }}{% endcapture %}
                  {% capture img_class %}product-featured-img{% endcapture %}
                  {% capture zoom_img_id %}FeaturedImageZoom-{{ section.id }}-{{ media.id }}{% endcapture %}
                  {% capture img_wrapper_id %}{{ zoom_img_id }}-wrapper{% endcapture %}
                  {%- assign img_url = media | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
                  {% include 'image-style' with small_style: true, width: 530, height: 530, wrapper_id: img_wrapper_id, img_id: img_id %}
                  <div id="{{ img_wrapper_id }}" class="product-single__photo-wrapper small--hide js">
                      <div id="{{ zoom_img_id }}" style="padding-top:{{ 1 | divided_by: media.aspect_ratio | times: 100}}%;position: relative;overflow: hidden;" class="product-single__photo-{{ section.id }}{% if enable_zoom %} js-zoom-enabled{% endif %}{% if product.images.size > 1 %} product-single__photo--has-thumbnails{% endif %}{% unless featured_image == media %} hide{% endunless %}" data-image-id="{{ media.id }}"{% if enable_zoom %} data-zoom="{{ media | img_url: product_image_zoom_size, scale: product_image_scale }}"{% endif %}>
                      <img id="{{ img_id }}" 
                          loading="lazy" 
                          class="feature-row__image {{ img_class }} lazyload{% unless featured_image == media %} lazypreload{% endunless %}"
                          src="{{ media | img_url: '300x300' }}"
                          data-src="{{ img_url }}"
                          data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 2048]"
                          data-aspectratio="{{ media.aspect_ratio }}"
                          data-sizes="auto"
                          alt="{{ media.alt | escape }}">
                      </div>
                  </div>            
              {% endcase %}
          {% endfor %}
  
        <noscript>
          {% capture product_image_size %}530x{% endcapture %}
          <img src="{{ featured_image | img_url: product_image_size, scale: product_image_scale }}" loading="lazy" alt="{{ featured_image.alt }}" id="FeaturedImage-{{ section.id }}" class="product-featured-img" style="max-width: 530px;">
        </noscript>
  
        {%- liquid
          if product.images.size > 1
            assign enable_thumbnail_slides = true
          endif 
        -%}
  
        <div class="thumbnails-wrapper {% if product.images.size == 1 %}medium-up--hide{% endif %}{% if enable_thumbnail_slides == true %} thumbnails-slider--active{% endif %}">
          {% if enable_thumbnail_slides == true %}
          <button type="button" class="btn btn--link small--hide thumbnails-slider__btn thumbnails-slider__prev thumbnails-slider__prev--{{ section.id }}">
            {% include 'icon-chevron-left' %}
            <span class="icon__fallback-text">{{ 'sections.slideshow.prev_slide' | t }}</span>
          </button>
          {% endif %}
          {%- if videos.size > 0 -%}
          <script>$(document).on('ready', function() {$('.thumbnails-slider--active .responsive').slick('slickGoTo', {{videos.size}})});</script>
          {%- endif -%}
              <ul class="grid responsive slider grid--uniform product-single__thumbnails product-single__thumbnails-{{ section.id }}">
                {%- if videos.size > 0 -%}
                  {%- for video in videos -%}
                    <li class="grid__item {% unless enable_thumbnail_slides == true %}{{ product_thumbnail_width }} {% endunless %}{% if product.images.size > 1 %}product-single__thumbnails-item{% endif %} js">
                    {%comment%}{{ media | external_video_tag }}{%endcomment%}
                      <a href="#" class="text-link product-single__thumbnail product-single__thumbnail--{{ section.id }} product-video" data-thumbnail-id="{{ video.id }}" {% if enable_zoom %}data-zoom="{{ video.src | img_url: product_image_zoom_size, scale: product_image_scale }}"{% endif %}>
                        <div class="product-single__thumbnail-image medium-up--hide video-media" style="height:230px;display: flex;align-items: center;justify-content: center;">{{ video | external_video_tag }}</div>
                        <img src="{{ 'play-button.png' | asset_img_url: '50x' }}" style="position: fixed; margin: 0 auto; top: 25%; left: 55px;" />
                        <img loading="lazy" class="product-single__thumbnail-image small--hide" src="{{ video.preview_image | img_url: 'compact' }}" alt="{{ video.alt | escape }}">                            
                      </a>
                    </li>
                  {%- endfor -%}
                {%- endif -%}
                      {% for media in product.media %}
                          {% case media.media_type %}
                          {% when 'image' %}
                              <li class="grid__item {% unless enable_thumbnail_slides == true %}{{ product_thumbnail_width }} {% endunless %}{% if product.images.size > 1 %}product-single__thumbnails-item{% endif %} js">
                              <a href="{{ media.src | img_url: product_image_zoom_size, scale: product_image_scale }}"
                                      class="text-link product-single__thumbnail product-single__thumbnail--{{ section.id }}"
                                      data-thumbnail-id="{{ media.id }}"
                                      {% if enable_zoom %}data-zoom="{{ media.src | img_url: product_image_zoom_size, scale: product_image_scale }}"{% endif %}>
                                      <img class="product-single__thumbnail-image medium-up--hide" loading="lazy" src="{{ media.src | img_url: '480x480', scale: 2 }}" alt="{{ media.alt | escape }}">
                                      <img class="product-single__thumbnail-image small--hide" loading="lazy" src="{{ media.src | img_url: 'compact' }}" alt="{{ media.alt | escape }}">
                                      </a>
                                  </li>                            
                          {% endcase %}            
                      {% endfor %}
              </ul>        
          {% if enable_thumbnail_slides == true %}
            <button type="button" class="btn btn--link small--hide thumbnails-slider__btn thumbnails-slider__next thumbnails-slider__next--{{ section.id }}">
              {% include 'icon-chevron-right' %}
              <span class="icon__fallback-text">{{ 'sections.slideshow.next_slide' | t }}</span>
            </button>
          {% endif %}
        </div>
          {%- if product.metafields.link.customer-review-photo -%}
            {%- render 'customer-review-photo' with product -%}
          {%- endif -%}
            {%- include 'getsocial-inline-bar' -%}
          <!-- Swym button on product pages -->
    </div>
      <div class="grid__item medium-up--one-half">
        <div class="product-single__meta">
          <h1 itemprop="name" class="product-single__title heading">
            {%- if product.metafields.text.product-h1 -%}
              {{ product.metafields.text.product-h1 }}
            {%- else -%}
              {{ product.title }}
            {%- endif -%}
          </h1>
  
          {% if section.settings.show_vendor %}
            <p itemprop="brand" class="product-single__vendor">{{ product.vendor }}</p>
          {% endif %}
  
          <div itemprop="offers" itemscope itemtype="http://schema.org/Offer">
            <meta itemprop="priceCurrency" content="USD">
            <meta itemprop="itemCondition" itemtype="http://schema.org/OfferItemCondition" content="http://schema.org/NewCondition" />
            <link itemprop="availability" href="http://schema.org/{% if product.available %}InStock{% else %}OutOfStock{% endif %}">
  
  {%- if  member_discount == true and product.type == 'Puzzle' -%}
              <p class="product-single__price product-single__price-{{ section.id }}{% unless current_variant.available %} product-price--sold-out{% endunless %}" data-discount="{{discount_amount}}">
                <s id="ComparePrice-{{ section.id }}" style="color: #ff8e00;">{{ current_variant.price | money }}</s>
                <span class="product-price__price product-price__price-{{ section.id }} product-price__sale product-price__sale--single">
                  <span id="ProductPrice-{{ section.id }}" itemprop="price" content="{{ current_variant.price | times: discount_amount | divided_by: 100.00 }}">
                    {{ current_variant.price | times: discount_amount | money	 }}
                  </span>
                  <span class="save_discount_pro">
                    <span class="per_discount">
                      20{{ 'products.product.on_save' | t }}
                    </span>
                </span>
              </p>
  {%- else -%}
             <p class="product-single__price product-single__price-{{ section.id }}{% unless current_variant.available %} product-price--sold-out{% endunless %}">
              {% if current_variant.compare_at_price > current_variant.price %}
                <span class="visually-hidden">{{ 'products.product.regular_price' | t }}</span>
                <s id="ComparePrice-{{ section.id }}">{{ current_variant.compare_at_price | money }}</s>
                <span class="product-price__price product-price__price-{{ section.id }} product-price__sale product-price__sale--single">
                  <span id="ProductPrice-{{ section.id }}"
                        itemprop="price" content="{{ current_variant.price | times: discount_amount | divided_by: 100.00 }}">
                    {{ current_variant.price | times: discount_amount | money	 }}
                  </span>
                  {% if section.settings.show_saved_amount %}
                  <!-- Dscount price in percentage  -->
                  <span class="save_discount_pro">
                  {% if current_variant.compare_at_price > current_variant.price %}
                    {% if settings.show_discount_amount == 'percent_save' %}
                    <span class="per_discount">
                      {% assign money_pro_format = shop.money_format | strip_html | json %}
                
                      {% if money_pro_format contains "{{amount_no_decimals}}" or money_pro_format contains "{{amount_no_decimals_with_comma_separator}}" %}
                        {{ current_variant.compare_at_price  | minus: current_variant.price | times: discount_amount  | times: 100.0 | divided_by: current_variant.compare_at_price | times: 100 | money_without_currency | replace:",","." | replace: '.0', '' }}{{ 'products.product.on_save' | t }}
                      {% elsif money_pro_format contains "{{amount_with_comma_separator}}" %}
